{{! ========================= Image/Icon Partial ============================ }}
{{!-- 
  {{> icon_image this}}
--}}

{{#*inline "icon_image" this}}
  <span {{#if (and this.title (or this.icon this.image))}}class="me-1"{{/if}}>
    {{#if this.image}}<img width=20 height=20 src="{{this.image}}">{{/if}}
    {{#if this.icon}}
      {{#unless (starts_with this.icon '<')}}
        {{icon_img this.icon}}
      {{else}}
        {{{this.icon}}}
      {{/unless}}
    {{/if}}
  </span>
{{/inline}}

{{! _________________________________________________________________________ }}


{{! =========================== Submenu Partial ============================= }}
{{!-- 
  {{> submenu this}}
--}}

{{#*inline "submenu" submenu}}
<div class="dropdown-menu">
  {{#each submenu as |item itemId|}}
    {{#unless item.submenu}}
      <a class="dropdown-item" href="{{link}}">
    {{else}}
      <a class="dropdown-item dropdown-toggle" href="{{link}}" data-bs-toggle="dropdown" data-bs-auto-close="false" role="button" aria-expanded="false" >
    {{/unless}}
          {{item.title}}
        {{#if item.badge}}
          <span class="badge badge-sm bg-{{item.badge_color}}{{#if item.badge_upper}} text-uppercase{{/if}} ms-auto">{{item.badge}}</span>
        {{/if}}
      </a>
      {{#if item.submenu}}{{> submenu item}}{{/if}}
  {{/each}}
</div>
{{/inline}}

{{! _________________________________________________________________________ }}


